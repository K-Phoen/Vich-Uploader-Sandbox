<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>{% block title %}VichUploaderBundle sandbox{% endblock %}</title>

    {% stylesheets
      '@KPhoenSandboxBundle/Resources/public/css/bootstrap.css'
      '@KPhoenSandboxBundle/Resources/public/css/font-awesome.min.css'
      '@KPhoenSandboxBundle/Resources/public/css/landing-page.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    <link href='http://fonts.googleapis.com/css?family=Lato:100,300,400,700,900,100italic,300italic,400italic,700italic,900italic' rel='stylesheet' type='text/css' />
</head>
<body>
<nav class="navbar navbar-default navbar-fixed-top" role="navigation">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                <span class="sr-only">toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{ path('homepage') }}">VichUploaderBundle sandbox</a>
        </div>

        <!-- collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse navbar-right navbar-ex1-collapse">
            <ul class="nav navbar-nav">
                <li><a href="{{ path('details_doctrine') }}">Doctrine</a></li>
                <li><a href="{{ path('details_propel') }}">Propel</a></li>
            </ul>
        </div>
        <!-- /.navbar-collapse -->
    </div>
    <!-- /.container -->
</nav>

<div class="intro-header">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="intro-message">
                    <h1>VichUploaderBundle sandbox</h1>
                    <h3>For... you know... stuff.</h3>

                    <hr class="intro-divider">

                    <ul class="list-inline intro-social-buttons">
                        <li>
                            <a href="https://github.com/K-Phoen/Vich-Uploader-Sandbox/" class="btn btn-default btn-lg"><i class="fa fa-github fa-fw"></i> <span class="network-name">GitHub</span></a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div><!-- /.container -->
</div><!-- /.intro-header -->

{% block body %}{% endblock %}

<div class="banner">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <h2>Want more links?</h2>
            </div>

            <div class="col-lg-6">
                <ul class="list-inline banner-social-buttons">
                    <li>
                        <a href="https://github.com/K-Phoen/Vich-Uploader-Sandbox/" class="btn btn-default btn-lg"><i class="fa fa-github fa-fw"></i> <span class="network-name">Sandbox GitHub</span></a>
                    </li>
                    <li>
                        <a href="https://github.com/dustin10/VichUploaderBundle/" class="btn btn-default btn-lg"><i class="fa fa-github fa-fw"></i> <span class="network-name">Bundle GitHub</span></a>
                    </li>
                </ul>
            </div>
        </div>
    </div><!-- /.container -->
</div><!-- /.banner -->

<footer>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <p class="copyright text-muted small">Design by <a href="https://github.com/IronSummitMedia/startbootstrap">IronSummitMedia</a></p>
            </div>
        </div>
    </div>
</footer>

<!-- JavaScript -->
 {% javascripts
   '@KPhoenSandboxBundle/Resources/public/js/jquery-1.10.2.js'
   '@KPhoenSandboxBundle/Resources/public/js/bootstrap.js'
  %}
 <script src="{{ asset_url }}"></script>
 {% endjavascripts %}
</body>
</html>
